{
	"name": "date-conversion",
	"displayName": "Date Conversion",
	"description": "Convert IOS, RFC, unixtime, etc. into readable form.",
	"icon": "resources/icon.png",
	"version": "1.0.0",
	"publisher": "CatTherapy",
	"engines": {
		"vscode": "^1.48.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/HayatoKudou/vscode-date-conversion"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
